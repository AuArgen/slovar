# .gitignore for Python Django

# Ignore virtual environment
venv/

# Ignore database
*.sqlite3
*.db

# Ignore static files
/staticfiles/

# Ignore media files
/media/

# Ignore environment configuration
.env

# Ignore compiled Python files
__pycache__/
*.pyc
*.pyo
*.pyd

# Ignore local development settings
settings_local.py

# Ignore logs and other generated files
logs/
*.log

# Ignore coverage reports
htmlcov/

# Ignore IDE and editor-specific files
.vscode/
.idea/
*.xml
*.sublime-project
*.sublime-workspace

# Ignore system files
.DS_Store
Thumbs.db
